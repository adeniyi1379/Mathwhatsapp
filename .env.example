# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_WHATSAPP_NUMBER=whatsapp:+14155238886

# SMS Configuration (Termii)
TERMII_API_KEY=your_termii_api_key
TERMII_SENDER_ID=SolveWithMe

# Database Configuration (Supabase)
DATABASE_URL=postgresql://username:password@host:port/database

# OpenAI Configuration (optional, for advanced AI features)
OPENAI_API_KEY=your_openai_api_key

# Analytics Configuration (Retool)
RETOOL_WEBHOOK_URL=https://your-retool-app.retool.com/api/webhook

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Feature Flags
ENABLE_SMS_NOTIFICATIONS=true
ENABLE_VOICE_PROCESSING=true
ENABLE_IMAGE_PROCESSING=true

# Scheduling
ENABLE_SCHEDULED_TASKS=true

# Environment
ENVIRONMENT=development

# Railway Port (automatically set by Railway)
PORT=8000
